<?xml version="1.0"?>
<configuration>
  
  <appSettings>

    <add key="FileNameDebug" value=".jeza.ioFTPD.Debug"/>
    <add key="FileNameRace" value=".jeza.ioFTPD.Race"/>
    <add key="FileNameIoFtpdMessage" value=".ioFTPD.Message"/>
    <add key="FileNameInternalLog" value="e:\\server\\ioFTPD\\logs\\jeza.ioFTPD.log"/>

    <add key="FileExtensionCrc32Failed" value=".bad"/>
    <add key="FileExtensionMissing" value=".missing"/>
    <add key="FileExtensionSfv" value=".sfv"/>
    <add key="FileExtensionMp3" value=".mp3"/>
    <add key="FileExtensionZip" value=".zip"/>
    <add key="FileExtensionNfo" value=".nfo"/>
    <add key="FileExtensionDiz" value=".diz"/>

    <add key="SkipPath" value="/_HDD/ /private/ /groups/ /nocheck/ /request/nocheck/ /fonts/"/>
    <add key="SkipFileExtension" value="jpg,jpeg,cue,txt,tcl,itcl,ini,cfg,m3u,avi,mpg,mpeg,vob"/>

    <add key="DeleteCrc32FailedFiles" value="true"/>
    <add key="ExtractNfoFromZip" value="true"/>
    <add key="WriteStatsToMesasageFileWhenRace" value="true"/>
    <add key="WriteStatsToMesasageFileWhenComplete" value="true"/>

    <add key="MaxNumberOfUserStats" value="30"/>
    <add key="MaxNumberOfGroupStats" value="30"/>

    <add key="LogToIoFtpdSfv" value="true"/>
    <add key="LogToIoFtpdUpdate" value="true"/>
    <add key="LogToIoFtpdUpdateMp3" value="true"/>
    <add key="LogToIoFtpdRace" value="true"/>
    <add key="LogToIoFtpdHalfway" value="true"/>
    <add key="LogToIoFtpdLeadUser" value="true"/>
    <add key="LogToIoFtpdLeadGroup" value="true"/>
    <add key="LogToIoFtpdComplete" value="true"/>
    <add key="LogToIoFtpdUserStatsHead" value="true"/>
    <add key="LogToIoFtpdUserStatsBody" value="true"/>
    <add key="LogToIoFtpdGroupStatsHead" value="true"/>
    <add key="LogToIoFtpdGroupStatsBody" value="true"/>

    <add key="LogToIoFtpdUserStatsBodyMaxNumber" value="3"/>
    <add key="LogToIoFtpdGroupStatsBodyMaxNumber" value="1"/>
    <add key="LogToIoFtpdHalfwayMinFiles" value="10"/>

    <add key="LogToInternalSfv" value="true"/>
    <add key="LogToInternalUpdate" value="true"/>
    <add key="LogToInternalUpdateMp3" value="true"/>
    <add key="LogToInternalRace" value="true"/>
    <add key="LogToInternalHalfway" value="true"/>
    <add key="LogToInternalLeadUser" value="true"/>
    <add key="LogToInternalLeadGroup" value="true"/>
    <add key="LogToInternalComplete" value="true"/>
    <add key="LogToInternalUserStatsHead" value="true"/>
    <add key="LogToInternalUserStatsBody" value="true"/>
    <add key="LogToInternalGroupStatsHead" value="true"/>
    <add key="LogToInternalGroupStatsBody" value="true"/>

    <add key="LogToInternalUserStatsBodyMaxNumber" value="3"/>
    <add key="LogToInternalGroupStatsBodyMaxNumber" value="1"/>
    <add key="LogToInternalHalfwayMinFiles" value="10"/>

    <!--
    The format used for output of autoformated bytes uploaded.
    {0}{1} : 123MB
    {0} {1}: 123 MB
    {0}_{1}: 123_MB
    -->
    <add key="FormatedBytes" value="{0}{1}"/>

    <!--
    Rescan
    -->
    <add key="ClientCrc32Head" value=" Expected Actual   Status   FileName "/>
    <add key="ClientCrc32Body" value=" {1,-8} {2,-8} {3,-8} {0,-60:B60}¤FileName ExpectedCrc32 ActualCrc32 Status"/>
    <add key="ClientCrc32Foot" value="{4}TotalFiles...: {0}{4}MissingFiles.: {1}{4}OkFiles......: {2}{4}FailedFiles..: {3}¤TotalFiles MissingFiles OkFiles FailedFiles NewLine"/>

    <add key="ClientHead" value=",----------=[ ZipScript by Jeza ]=-----------------------------------------------------------,"/>
    <add key="ClientFileName" value="| File.....: {0,-80:B79}|¤FileName"/>
    <add key="ClientFileNameOk" value="| File OK..: {0,-80:B79}|¤FileName"/>
    <add key="ClientFileNameSkip" value="| Skip.....: {0,-80:B79}|¤FileName"/>
    <add key="ClientFileNameBadCrc" value="| Bad CRC..: {0,-80:B79}|¤FileName"/>
    <add key="ClientFileNameNoDiz" value="| No DIZ...: {0,-80:B79}|¤FileName"/>
    <add key="ClientFileNameSfv" value="| SFV......: {0,-80:B79}|¤FileName"/>
    <add key="ClientFileNameSfvExists" value="| Deleting.: SFV allready exists!                                                            |"/>
    <add key="ClientFileNameSfvFirst" value="| Deleting.: Upload SFV first!                                                               |"/>
    <add key="ClientMp3InfoHead" value="|----------=[ Mp3 Info          ]=-----------------------------------------------------------|"/>

    <add key="ClientMp3Info" value="| Track....: {0,-80:B79}|{6}| Title....: {1,-80:B79}|{6}| Artist...: {2,-80:B79}|{6}| Album....: {3,-80:B79}|{6}| Year.....: {4,-80:B79}|{6}| Genre....: {5,-80:B79}|¤TrackNumber Title Artist Album Year Genre NewLine"/>

    <add key="ClientStatsUsersHead" value="|----------=[ User Stats        ]=-----------------------------------------------------------|"/>
    <add key="ClientStatsUsers" value="| {0,10:B2}. {1,-23:B23} {2,6:B6} {3,6:B6}kBit/s {4,4:B4}F                              |¤Possition UserName FormatBytesUploaded AverageSpeed FilesUploaded"/>
    <add key="ClientStatsGroupsHead" value="|----------=[ Group Stats       ]=-----------------------------------------------------------|"/>
    <add key="ClientStatsGroups" value="| {0,10:B2}. {1,-23:B23} {2,6:B6} {3,6:B6}kBit/s {4,4:B4}F                              |¤Possition GroupName FormatBytesUploaded AverageSpeed FilesUploaded"/>
    <add key="ClientFoot" value="'---------------------------------------------------------------------=[ {0,3:B3}/{1,-3:B3} ]=----------'¤TotalFilesUploaded TotalFilesExpected"/>
    <add key="ClientFootProgressBar" value="'----------=[ {2,17:B17} ]=------------------------------------=[ {0,3:B3}/{1,-3:B3} ]=----------'¤TotalFilesUploaded TotalFilesExpected ProgressBar"/>

    <add key="MessageMp3InfoHead" value="|----------=[ Mp3 Info          ]=-----------------------------------------------------------|"/>

    <add key="MessageMp3Info" value="| Artist...: {0,-80:B79}|{4}| Album....: {1,-80:B79}|{4}| Year.....: {2,-80:B79}|{4}| Genre....: {3,-80:B79}|¤Artist Album Year Genre NewLine"/>

    <add key="MessageStatsUsersHead" value="|----------=[ User Stats        ]=-----------------------------------------------------------|"/>
    <add key="MessageStatsUsers" value="| {0,10:B2}. {1,-23:B23} {2,6:B6} {3,6:B6}kBit/s {4,4:B4}F                              |¤Possition UserName FormatBytesUploaded AverageSpeed FilesUploaded"/>
    <add key="MessageStatsGroupsHead" value="|----------=[ Group Stats       ]=-----------------------------------------------------------|"/>
    <add key="MessageStatsGroups" value="| {0,10:B2}. {1,-23:B23} {2,6:B6} {3,6:B6}kBit/s {4,4:B4}F                              |¤Possition GroupName FormatBytesUploaded AverageSpeed FilesUploaded"/>
    <add key="MessageFoot" value="'---------------------------------------------------------------------=[ {0,3:B3}/{1,-3:B3} ]=----------'¤TotalFilesUploaded TotalFilesExpected"/>

    <add key="LogLineIoFtpdSfv" value="SFV: &quot;{0}&quot; &quot;{4}03{3}sfv{3}{4} {3}{1}{3} [{3}{2}{3}F]&quot;¤UploadVirtualPath ReleaseName TotalFilesExpected ircBold ircColor"/>
    <add key="LogLineIoFtpdUpdate" value="UPDATE: &quot;{0}&quot; &quot;{4}04{3}info{3}{4} {3}{1}{3} [{3}{5}{3} in {3}{2}F{3}]&quot;¤UploadVirtualPath ReleaseName TotalFilesExpected ircBold ircColor FormatTotalBytesExpected"/>
    <add key="LogLineIoFtpdUpdateMp3" value="UPDATE: &quot;{0}&quot; &quot;{4}04{3}info{3}{4} {3}{1}{3} [{3}{5}{3} in {3}{2}F{3} @ {3}{6}{7}{6}{3} {3}{8}{3}]&quot;¤UploadVirtualPath ReleaseName TotalFilesExpected ircBold ircColor FormatTotalBytesExpected ircUnderline Genre Year"/>
    <add key="LogLineIoFtpdHalfway" value="HALFWAY: &quot;{0}&quot; &quot;{4}04{3}halfway{3}{4} {3}{1}{3} [{3}{5}{3}Groups / {3}{2}{3}Users @ {3}{6}kBps{3}]&quot;¤UploadVirtualPath ReleaseName TotalUsersRacing ircBold ircColor TotalGroupsRacing TotalAvarageSpeed"/>
    <add key="LogLineIoFtpdRace" value="RACE: &quot;{0}&quot; &quot;{3}04{2}race{2}{3} {2}{1}{2} [{2}{4}{2}/{5} @ {2}{6}{2}kBps]&quot;¤UploadVirtualPath ReleaseName ircBold ircColor UserName GroupName AverageSpeed"/>
    <add key="LogLineIoFtpdLeadUser" value="LEADER: &quot;{0}&quot; &quot;{3}04{2}leader{2}{3} {2}{1}{2} [{2}{4}{2}/{5} with {7} @ {2}{6}{2}kBps]&quot;¤UploadVirtualPath ReleaseName ircBold ircColor UserName GroupName AverageSpeed FormatBytesUploaded"/>
    <add key="LogLineIoFtpdLeadGroup" value="LEADER: &quot;{0}&quot; &quot;{3}04{2}leader{2}{3} {2}{1}{2} [{2}{4}{2} with {6} @ {2}{5}{2}kBps]&quot;¤UploadVirtualPath ReleaseName ircBold ircColor GroupName AverageSpeed FormatBytesUploaded"/>
    <add key="LogLineIoFtpdComplete" value="COMPLETE: &quot;{0}&quot; &quot;{4}12{3}complete{3}{4} {3}{1}{3} [{3}{5}{3} in {3}{2}F{3} @ {3}{6}kBps{3}]&quot;¤UploadVirtualPath ReleaseName TotalFilesExpected ircBold ircColor FormatBytesUploaded TotalAvarageSpeed"/>
    <add key="LogLineIoFtpdUserStatsHead" value="STATS: &quot;{0}&quot; &quot;User Stats:&quot;¤UploadVirtualPath"/>
    <add key="LogLineIoFtpdUserStatsBody" value="STATS: &quot;{0}&quot; &quot;{1,2:B2}. {2,-14:B14} {3,6:B6} {4,6:B6}kBit/s {5,4:B4}F&quot;¤UploadVirtualPath Possition UserName FormatBytesUploaded AverageSpeed FilesUploaded"/>
    <add key="LogLineIoFtpdGroupStatsHead" value="STATS: &quot;{0}&quot; &quot;Group Stats:&quot;¤UploadVirtualPath"/>
    <add key="LogLineIoFtpdGroupStatsBody" value="STATS: &quot;{0}&quot; &quot;{1,2:B2}. {2,-14:B14} {3,6:B6} {4,6:B6}kBit/s {5,4:B4}F&quot;¤UploadVirtualPath Possition GroupName FormatBytesUploaded AverageSpeed FilesUploaded"/>

    <add key="LogLineInternalSfv" value="SFV: &quot;{0}&quot; &quot;{4}03{3}sfv{3}{4} {3}{1}{3} [{3}{2}{3}F]&quot;¤UploadVirtualPath ReleaseName TotalFilesExpected ircBold ircColor"/>
    <add key="LogLineInternalUpdate" value="UPDATE: &quot;{0}&quot; &quot;{4}04{3}info{3}{4} {3}{1}{3} [{3}{5}{3} in {3}{2}F{3}]&quot;¤UploadVirtualPath ReleaseName TotalFilesExpected ircBold ircColor FormatTotalBytesExpected"/>
    <add key="LogLineInternalUpdateMp3" value="UPDATE: &quot;{0}&quot; &quot;{4}04{3}info{3}{4} {3}{1}{3} [{3}{5}{3} in {3}{2}F{3} @ {3}{6}{7}{6}{3} {3}{8}{3}]&quot;¤UploadVirtualPath ReleaseName TotalFilesExpected ircBold ircColor FormatTotalBytesExpected ircUnderline Genre Year"/>
    <add key="LogLineInternalRace" value="RACE: &quot;{0}&quot; &quot;{3}04{2}race{2}{3} {2}{1}{2} [{2}{4}{2}/{5} @ {2}{6}{2}kBps]&quot;¤UploadVirtualPath ReleaseName ircBold ircColor UserName GroupName AverageSpeed"/>
    <add key="LogLineInternalLeadUser" value="LEADER: &quot;{0}&quot; &quot;{3}04{2}leader{2}{3} {2}{1}{2} [{2}{4}{2}/{5} with {7} @ {2}{6}{2}kBps]&quot;¤UploadVirtualPath ReleaseName ircBold ircColor UserName GroupName AverageSpeed FormatBytesUploaded"/>
    <add key="LogLineInternalLeadGroup" value="LEADER: &quot;{0}&quot; &quot;{3}04{2}leader{2}{3} {2}{1}{2} [{2}{4}{2} with {6} @ {2}{5}{2}kBps]&quot;¤UploadVirtualPath ReleaseName ircBold ircColor GroupName AverageSpeed FormatBytesUploaded"/>
    <add key="LogLineInternalHalfway" value="HALFWAY: &quot;{0}&quot; &quot;{4}04{3}halfway{3}{4} {3}{1}{3} [{3}{5}{3}Groups / {3}{2}{3}Users @ {3}{6}kBps{3}]&quot;¤UploadVirtualPath ReleaseName TotalUsersRacing ircBold ircColor TotalGroupsRacing TotalAvarageSpeed"/>
    <add key="LogLineInternalComplete" value="COMPLETE: &quot;{0}&quot; &quot;{4}12{3}complete{3}{4} {3}{1}{3} [{3}{5}{3} in {3}{2}F{3} @ {3}{6}kBps{3}]&quot;¤UploadVirtualPath ReleaseName TotalFilesExpected ircBold ircColor FormatBytesUploaded TotalAvarageSpeed"/>
    <add key="LogLineInternalUserStatsHead" value="STATS: &quot;{0}&quot; &quot;User Stats:&quot;¤UploadVirtualPath"/>
    <add key="LogLineInternalUserStatsBody" value="STATS: &quot;{0}&quot; &quot;{1,2:B2}. {2,-14:B14} {3,6:B6} {4,6:B6}kBit/s {5,4:B4}F&quot;¤UploadVirtualPath Possition UserName FormatBytesUploaded AverageSpeed FilesUploaded"/>
    <add key="LogLineInternalGroupStatsHead" value="STATS: &quot;{0}&quot; &quot;Group Stats:&quot;¤UploadVirtualPath"/>
    <add key="LogLineInternalGroupStatsBody" value="STATS: &quot;{0}&quot; &quot;{1,2:B2}. {2,-14:B14} {3,6:B6} {4,6:B6}kBit/s {5,4:B4}F&quot;¤UploadVirtualPath Possition GroupName FormatBytesUploaded AverageSpeed FilesUploaded"/>

    <add key="ProgressBarLength" value="17"/>
    <add key="ProgressBarCharFilled" value="#"/>
    <add key="ProgressBarCharMissing" value="-"/>

    <add key="Mp3SortPathArtist" value="c:\\sorted\\by.artist"/>
    <add key="Mp3SortPathGenre" value="c:\\sorted\\by.genre"/>
    <add key="Mp3SortPathYear" value="c:\\sorted\\by.year"/>

    <!--
    The tag cannot contain the following characters : \ / : * ? " < > |
    -->
    <!-- delete all files that start with this string in race folder -->
    <add key="TagCleanUpString" value="]-["/>
    <add key="TagIncomplete" value="]-[iNCOMPLETE]-[ {0}% ]-[{1}FiLE(s) of {2}FiLE(s)]-[iNCOMPLETE]-[¤PercentComplete TotalFilesUploaded TotalFilesExpected"/>
    <add key="TagComplete" value="]-[Complete]-[{2}%]-[ {0} - {1}F]-[¤FormatBytesUploaded TotalFilesUploaded PercentComplete"/>
    <add key="TagCompleteMp3" value="]-[Complete]-[{2}%]-[ {0} - {1}F]-[{4} - {3}]-[¤FormatBytesUploaded TotalFilesUploaded PercentComplete Year Genre"/>
    <add key="TagIncompleteLink" value="[iNCOMPLETE]-{0}¤ReleaseName"/>
    <add key="TagIncompleteLinkDiscNumber" value="[iNCOMPLETE]-{0}_{1}¤ReleaseName DiscNumber"/>
    <add key="TagIncompleteLinkChMod" value="755"/>

  </appSettings>
  
  <system.serviceModel>
    <services>
      <!-- This section is optional with the new configuration model
           introduced in .NET Framework 4. -->
      <service name="jeza.ioFTPD.Manager.ManagerService"
               behaviorConfiguration="ManagerServiceBehavior">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8000/ioFTPDManager/service"/>
          </baseAddresses>
        </host>
        <!-- this endpoint is exposed at the base address provided by host: http://localhost:8000/ioFTPDManager/service  -->
        <endpoint address=""
                  binding="wsHttpBinding"
                  contract="jeza.ioFTPD.Manager.IManager" />
        <!-- the mex endpoint is exposed at http://localhost:8000/ioFTPDManager/service/mex -->
        <endpoint address="mex"
                  binding="mexHttpBinding"
                  contract="IMetadataExchange" />
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="ManagerServiceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="False"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>
